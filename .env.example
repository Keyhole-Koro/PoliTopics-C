# AWS
AWS_REGION=ap-northeast-3
TABLE_NAME=politopics
ERROR_BUCKET=politopics-error-logs

# National Diet API
NATIONAL_DIET_API_ENDPOINT=https://kokkai.ndl.go.jp/api/meeting

# LLM Provider (gemini | groq)
LLM_PROVIDER=gemini

# Gemini
GEMINI_MODEL_NAME=YOUR_MODEL_NAME_HERE # optional, e.g., "gemini-2.5-flash"
GEMINI_API_KEY=YOUR_KEY_HERE
GEMINI_TIMEOUT_MS=

# LLM concurrency settings (optional)
LLM_RPS=
LLM_BURST=
LLM_REDUCE_CONCURRENCY=
LLM_CHUNK_CONCURRENCY=

# LLM minute/day/token budgets (optional)
LLM_RPM=            # requests per minute (total)
LLM_RPD=            # requests per day (UTC)
LLM_TPM=            # tokens per minute (input+output)
LLM_TPM_STRICT=     # true to try reserving tokens pre-call when supported

# Preferred cross-cutting budgets via middleware wrapper
LLM_BUDGET_RPM=
LLM_BUDGET_RPD=
LLM_BUDGET_TPM=
LLM_BUDGET_TPM_STRICT=

GEMINI_RETRY_MAX=
GEMINI_RETRY_BASE_MS=
GEMINI_RETRY_MAX_MS=

# Groq (OpenAI-compatible)
GROQ_MODEL_NAME=llama-3.1-70b-versatile
GROQ_API_KEY=
GROQ_BASE_URL=https://api.groq.com/openai/v1
GROQ_TIMEOUT_MS=
GROQ_RPM=
GROQ_RPD=
GROQ_TPM=
GROQ_TPM_STRICT=
GROQ_RPS=
GROQ_BURST=
GROQ_MAX_CONCURRENCY=
GROQ_RETRY_MAX=
GROQ_RETRY_BASE_MS=
GROQ_RETRY_MAX_MS=

# Data processing concurrency settings (optional)
REDUCE_GROUP_SIZE=
REDUCE_CONCURRENCY=
